<p id="notice"><%= notice %></p>

<h1>Doctors</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>User name</th>
      <th>Password</th>
      <th>Email</th>
      <th>Mobile no</th>
      <th>Specialist in</th>
      <th>Gender</th>
      <th>State</th>
      <th>City</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.first_name %></td>
        <td><%= doctor.last_name %></td>
        <td><%= doctor.user_name %></td>
        <td><%= doctor.password %></td>
        <td><%= doctor.email %></td>
        <td><%= doctor.mobile_no %></td>
        <td><%= doctor.specialist_in %></td>
        <td><%= doctor.gender %></td>
        <td><%= doctor.state %></td>
        <td><%= doctor.city %></td>
        <td><%= doctor.address %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
